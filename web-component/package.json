    "react-test-renderer": "16.9.0",
    "to-string-loader": "^1.1.6",
    "typescript": "^3.9.3",
    "webpack-cli": "^3.3.11",
    "ts-loader": "^6.2.2"
  },
  "eslintConfig": {
